{
  "require": {
    "craftcms/cms": "3.0.25",
    "vlucas/phpdotenv": "^2.4.0",
    "solspace/craft3-calendar": "2.0.6",
    "facebook/graph-sdk": "^5.6",
    "craftcms/ckeditor": "1.0.0-beta.2",
    "craftcms/redactor": "2.1.6",
    "ether/simplemap": "v3.3.4",
    "jmx2/pic-puller": "3.0.13.2",
    "dukt/videos": "2.0.4",
    "dolphiq/sitemap": "1.0.9"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
